IF


pancakes = int(input())
x = pancakes
if int(x) > 3:
 print ("Yum!")
else:
 print ("Still hungry!")


x=int(input())
if int(x) == 0:
 print ("Zero")
if int(x) > 0: 
 print ("Positive")
if int(x) < 0:
 print ("Negative")


age = int(input())
if age < 0:
 print ("You are a time traveller")
elif age < 18:
 print ("Too young to vote")
elif age >= 18: 
 print ("You can vote")

