mystring = input ()
x = chr(32)
print (mystring + "," + x + mystring + "," + x + "bo-b" + mystring[1:])
print ("banana-fana" + x + "fo-f" + mystring[1:])
print ("fee-fi-mo-m" + mystring[1:])
